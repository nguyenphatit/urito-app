{
  "extends": "next/core-web-vitals",
  "rules": {
    "prefer-const": "error",
    "curly": "error",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-else-return": "error",
    "no-unused-vars": "error",
    "eol-last": ["error", "always"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
